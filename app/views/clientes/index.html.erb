<%= titulo("Clientes") %> 
<p id="notice"><%= notice %></p>
<div class="panel-body2"  style="height: 95%;">
    <div class="row">
        <div class="col-md-4" >
          <%= link_to 'Nuevo', new_cliente_path,
            class: "btn btn-default btn-sm nuevo_cliente"  %>
        </div>
        <div class="col-md-8" align="right">
           <%= buscar(clientes_path,["identidad","nombres","apellidos"]) %>
        </div>
    </div>
    <%= tabla_panel_responsive(["Identidad","Nombres","Apellidos","Telefono"],@clientes)%>
    <%= paginador(@clientes,controller: :clientes, action: :index ) %>
</div>

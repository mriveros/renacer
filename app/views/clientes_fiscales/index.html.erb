<%= index_titulo %> 
<%= render partial: "/shared/flash_messages", flash: flash %> 
<div class="panel-body2" >
    <div class="row">
        <div class="col-sm-4 col-md-4" >
          <%= link_to 'Nuevo', new_cliente_fiscal_path,
            class: "btn btn-default btn-sm nuevo_cliente"  %>
        </div>
        <div class="col-md-8" align="right">
           <%= buscar(clientes_fiscales_path,["identidad","nombres","direccion"]) %>
        </div>
    </div>
    <%= tabla_panel_responsive([
            { title: "Id", width: "10%"},
            { title: "Identidad", width: "20%"},
            { title: "Nombres", width: "20%"},
            { title: "Direccion", width: "50%"},
        ],@clientes)%>
    <%= paginador(@clientes,controller: :clientes_fiscales, action: :index ) %>
</div>
